<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.suvrat.ShoppingWithFriends" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/shopping"
        android:label="@string/app_name"
        android:theme="@style/DarkTheme" >
        <activity
            android:name="com.example.suvrat.ShoppingWithFriends.RegisterScreen"
            android:label="RegisterScreen" >
        </activity>
        <activity
            android:name="com.example.suvrat.ShoppingWithFriends.Acceptfriends"
            android:label="Listfriends" >
        </activity>
        <activity
            android:name="com.example.suvrat.ShoppingWithFriends.AddFriends"
            android:label="AddFriends" >
        </activity>
        <activity
            android:name="com.example.suvrat.ShoppingWithFriends.MainActivity"
            android:label="HelloWorld" >
        </activity>
        <activity
            android:name="com.example.suvrat.ShoppingWithFriends.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>
